app:layout_constraintStart_toStartOf="parent"
 android:layout_marginTop="8dp"
    android:background="@color/blue"
    android:text="float left"
    tools:ignore="MissingConstraints" />
                <Button
                    android:id="@+id/bu2"
                    android:layout_width="128dp"
                    android:layout_height="wrap_content"
                    android:background="@color/red"
                    android:layout_marginTop="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:text="float Right"
                    tools:ignore="MissingConstraints" />
                <TextView
                    android:id="@+id/t1"
                    android:layout_width="match_parent"
                    android:layout_marginTop="8dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/green"
                    android:text="float Property"
                    android:textSize="25sp"
                   app:layout_constraintTop_toBottomOf="@+id/bu1"

                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="101dp"
                    tools:layout_editor_absoluteY="3dp" />
                <TextView
                    android:id="@+id/t11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="8dp"
                    android:text="The float property is used for positioning and layout on web pages.\n \n
The float property can have one of the following values:\n \n
left - The element floats to the left of its container\n
right- The element floats to the right of its container\n
none - The element does not float (will be displayed just where it occurs in the text). This is default\n
inherit - The element inherits the float value of its parent\n \n
In its simplest use, the float property can be used to wrap text around images."
                    app:layout_constraintTop_toBottomOf="@+id/t1"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="4dp" />
                <TextView
                    android:id="@+id/t111"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="8dp"
                    android:textColor="@color/teal"
                    android:text="Example"

                    app:layout_constraintTop_toBottomOf="@+id/t11"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="4dp" />
                <TextView
                    android:id="@+id/t1111"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="8dp"
                    android:text="img {\n float:right; \n }"
                    app:layout_constraintTop_toBottomOf="@+id/t111"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="4dp" />

                <View
                    android:id="@+id/v1"
                    android:layout_width="match_parent"
